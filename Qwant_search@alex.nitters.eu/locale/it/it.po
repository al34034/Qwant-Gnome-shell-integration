#Settings

#: prefs.js:70
#Needs logout label ("(Needs logout)")
msgid "(Nécéssite la déconnexion)"
msgstr "(Logout richiesto)"

#: prefs.js:70
#Setting to display the panel button ("Panel button")
msgid "Bouton Qwant dans la barre système"
msgstr "Tasto Qwant nella barra sistema"


#: prefs.js:85
#Setting to select search category ("Search category")
msgid "Catégorie de recherche"
msgstr "Categoria di ricerca"

#: prefs.js:98
#Setting to select search engine : Qwant, Qwant Junior, Qwant Lite... ("Search engine")
msgid "Moteur de recherche"
msgstr "Motore di ricerca"

#: prefs.js:105
#Label for suggestions part ("Suggestions")
msgid "Suggestions"
msgstr "Suggerimenti"

#: prefs.js:108
#Setting to completely disable suggestions ("Completely disable search without shortcuts (only search shortcuts will be enabled)")
msgid "Complètement désactiver la recherche sans raccourci (seuls les raccourcis de recherche seront activés)"
msgstr "Disattiva completamente la ricerca senza scorciatoie (saranno abilitate solo le scorciatoie di ricerca)"

#: prefs.js:113
#Setting to show suggestions ("Show suggestions (the search option will always be available)")
msgid "Afficher les suggestions (l'option recherche sera toujours présente)"
msgstr "Mostra suggerimenti (l'opzione di ricerca sarà sempre disponibile)"

#: prefs.js:123
#Setting to set max number of suggestions ("Maximum number of suggestions (set to 0 to disable)")
msgid "Nombre maximal de suggestions (laissez à 0 pour ne pas limiter)"
msgstr "Numero massimo di suggerimenti (lasciare 0 per non limitare)"

#: prefs.js:129
#Setting to set suggestions keyword ("Suggestions keyword (leave empty to always show)")
msgid "Raccourci pour les suggestions (laissez vide pour toujours montrer)"
msgstr "Scorciatoia per suggerimenti (lasciare vuoto per mostrare sempre)"

#: prefs.js:134
#Label for search keywords part ("Search keywords (leave empty to disable)")
msgid "Raccourcis pour la recherche (laissez vide pour désactiver)"
msgstr "Tasti di scelta rapida per la ricerca (lasciare vuoto per disabilitare)"

#: prefs.js:136
#Setting to completely disable search ("Disable search")
msgid "Complètement désactiver la recherche"
msgstr "Disattiva completamente la ricerca"
